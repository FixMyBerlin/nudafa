{
  "name": "@keystatic/templates-astro",
  "version": "0.0.17",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro)\" --write",
    "format:all": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "updatePackages:major": "npx taze major --includeLocked --write --exclude eslint && npm run updatePackages:minor && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.1",
    "@astrojs/markdoc": "^0.11.1",
    "@astrojs/mdx": "^3.1.2",
    "@astrojs/netlify": "^5.4.0",
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.9.0",
    "@fontsource-variable/open-sans": "^5.0.29",
    "@fontsource/roboto-slab": "^5.0.20",
    "@headlessui/react": "^2.1.2",
    "@headlessui/tailwindcss": "^0.2.1",
    "@heroicons/react": "^2.1.5",
    "@keystatic/astro": "^5.0.0",
    "@keystatic/core": "^0.5.27",
    "@nanostores/react": "^0.7.2",
    "@nanostores/router": "^0.15.0",
    "@turf/turf": "^7.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.11.5",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "heroicons-astro": "^1.0.4",
    "maplibre-gl": "^4.5.0",
    "nanostores": "^0.10.3",
    "pmtiles": "^3.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-map-gl": "^7.2.0-beta.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5"
  }
}
