{
  "name": "@keystatic/templates-astro",
  "version": "0.0.17",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "updatePackages:major": "nvm use && npx taze major --write && npm install",
    "updatePackages:minor": "nvm use && npx taze minor --write && npm install"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/markdoc": "^0.8.0",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/netlify": "^5.1.3",
    "@astrojs/react": "^3.0.7",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.8.0",
    "@fontsource-variable/open-sans": "^5.0.27",
    "@fontsource/roboto-slab": "^5.0.19",
    "@headlessui/react": "^1.7.18",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.1.1",
    "@keystatic/astro": "^5.0.0",
    "@keystatic/core": "^0.5.12",
    "@nanostores/react": "^0.7.2",
    "@nanostores/router": "^0.15.0",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.0.11",
    "astro": "^4.0.3",
    "clsx": "^2.1.0",
    "date-fns": "^3.4.0",
    "heroicons-astro": "^1.0.4",
    "nanostores": "^0.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14"
  }
}
