{
  "name": "@keystatic/templates-astro",
  "version": "0.0.17",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "generate-types": "astro sync --verbose && cat ./.astro/astro/content.d.ts",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint '{src,keystatic}/**/*.+(ts|js|tsx|astro)' --fix",
    "format": "prettier --ignore-path .gitignore '{src,keystatic}/**/*.+(ts|js|tsx|astro)' --write",
    "format:all": "prettier --ignore-path .gitignore '{src,keystatic}/**/*.+(ts|js|tsx|astro|mdx)' --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "updatePackages:major": "npx taze major --includeLocked --write && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install",
    "release": "gh pr create --base main --head develop --title \"Release $(date '+%Y-%m-%d')\" --body \"\"",
    "prepare": "husky",
    "transformMeasuresCsvToMdx": "node scripts/csvToJson.js ./data/measures/massnahmenliste.csv ./data/measures/massnahmenliste.json && node scripts/measureJsonToMdx.js && npm run format:all",
    "updateMeasuresGeometry": "node scripts/updateMeasuresGeometry.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdoc": "^0.11.5",
    "@astrojs/mdx": "^3.1.9",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@astrojs/ts-plugin": "^1.10.4",
    "@fontsource-variable/open-sans": "^5.1.0",
    "@fontsource/roboto-slab": "^5.1.0",
    "@headlessui/react": "^2.2.0",
    "@headlessui/tailwindcss": "^0.2.1",
    "@heroicons/react": "^2.1.5",
    "@keystatic/astro": "^5.0.3",
    "@keystatic/core": "^0.5.40",
    "@nanostores/react": "^0.8.0",
    "@nanostores/router": "^0.16.1",
    "@turf/turf": "^7.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^4.16.13",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "heroicons-astro": "^1.0.4",
    "maplibre-gl": "^4.7.1",
    "nanostores": "^0.11.3",
    "papaparse": "^5.4.1",
    "pmtiles": "^3.2.1",
    "react": "^18.3.1",
    "react-aria-components": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-map-gl": "^7.2.0-beta.1",
    "react-stately": "^3.33.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.9.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.15.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8"
  }
}
