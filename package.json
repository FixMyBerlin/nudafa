{
  "name": "@keystatic/templates-astro",
  "version": "0.0.17",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "updatePackages:major": "npx taze major --includeLocked --write && npm install",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/markdoc": "^0.11.0",
    "@astrojs/mdx": "^3.0.1",
    "@astrojs/netlify": "^5.2.1",
    "@astrojs/react": "^3.4.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.8.0",
    "@fontsource-variable/open-sans": "^5.0.29",
    "@fontsource/roboto-slab": "^5.0.20",
    "@headlessui/react": "^2.0.4",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.1.3",
    "@keystatic/astro": "^5.0.0",
    "@keystatic/core": "^0.5.17",
    "@nanostores/react": "^0.7.2",
    "@nanostores/router": "^0.15.0",
    "@turf/turf": "^7.0.0-alpha.116",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.9.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "heroicons-astro": "^1.0.4",
    "maplibre-gl": "^4.3.2",
    "nanostores": "^0.10.3",
    "pmtiles": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-map-gl": "^7.2.0-beta.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14"
  }
}
