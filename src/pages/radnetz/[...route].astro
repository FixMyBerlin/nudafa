---
import LayoutApp from '@components/layouts/LayoutApp.astro'
import { Radnetz } from '@components/page_radnetz/Radnetz'

export function getStaticPaths() {
  // This catch-all renders our React components which navigates with client side routing .
  // However the first page load will use the static generated page.
  // Props like the page title will only update on the initial page load or a page reload.
  //
  // const projects = await getCollection('subprojects')
  // return projects.map((entry) => ({
  //   params: { slug: entry.slug },
  //   props: { entry },
  // }))
  return [
    { params: { route: undefined }, props: { title: 'foo1' } },
    { params: { route: 'einleitung' }, props: { title: 'foo2' } },
    { params: { route: 'page2' }, props: { title: 'foo3' } },
  ]
}

const { title } = Astro.props
// const { entry } = Astro.props
// const { title, subTitle, topics, projectCommunes, projectPartners, start, end, funding } =
//   entry.data
// const { Content } = await entry.render()
---

<LayoutApp title={title}>
  <!-- Has to be `client:only`, not `client:load` to work around hydration missmatches. -->
  <Radnetz client:only />
</LayoutApp>
